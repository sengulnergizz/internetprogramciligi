        document.addEventListener("click", function (e) {
            if (e.target.classList.contains("view-btn")) {
                const card = e.target.closest(".col");
                const sehirID = card.id;
                const sehirIsmi = card.querySelector("text").textContent;

                const veri = detaylar[sehirID];

                modalTitle.textContent = sehirIsmi;

                // EÄŸer ÅŸehir detaylarÄ± yoksa
                if (!veri) {
                    modalText.textContent = "Bu ÅŸehir iÃ§in detaylÄ± bilgi henÃ¼z eklenmedi.";
                } else {
                    // AÃ§Ä±klamayÄ± yazdÄ±r
                    modalText.textContent = veri.aciklama;

                    
                    const eski = document.getElementById("yemekler");
                    if (eski) eski.remove();

                   
                    const yemeklerDiv = document.createElement("div");
                    yemeklerDiv.id = "yemekler";

                    veri.yemekler.forEach(yemek => {
                        yemeklerDiv.innerHTML += `
                    <div style="margin-top:10px;">
                        <h4>${yemek.ad}</h4>
                        <img src="${yemek.resim}" alt="${yemek.ad}" style="width:150px; border-radius:8px;">
                    </div>
                `;
                    });

                    modalText.parentElement.appendChild(yemeklerDiv);
                }

                modal.style.display = "block";
            }
        });

        closeBtn.onclick = () => modal.style.display = "none";
        window.onclick = (e) => { if (e.target == modal) modal.style.display = "none"; };

        document.querySelectorAll('.view-btn').forEach(btn => {
            btn.addEventListener('click', function (e) {
                e.preventDefault(); 
                
            });
        });



    </script>



</body>

