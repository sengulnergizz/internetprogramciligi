    <script>
        function normalizeTurkish(str) {
            return str
                .toLowerCase()
                .replace(/Ä±/g, "i")
                .replace(/ÄŸ/g, "g")
                .replace(/Ã¼/g, "u")
                .replace(/ÅŸ/g, "s")
                .replace(/Ã¶/g, "o")
                .replace(/Ã§/g, "c");
        }

        document.getElementById('searchForm').addEventListener('submit', function (e) {
            e.preventDefault();
            const query = normalizeTurkish(document.getElementById('searchInput').value.trim());
            const target = document.getElementById(query);
            if (target) {
                target.scrollIntoView({ behavior: 'smooth', block: 'center' });
            } else {
                alert('Bu ÅŸehir bulunamadÄ±!');
            }
        });
    </script>
    <button id="scrollTopBtn">â®</button>



    <script>
        
        const scrollBtn = document.getElementById('scrollTopBtn');

        // Sayfa aÅŸaÄŸÄ± kayÄ±nca butonu gÃ¶ster
        window.addEventListener('scroll', () => {
            if (window.scrollY > 300) {
                scrollBtn.style.display = 'block';
            } else {
                scrollBtn.style.display = 'none';
            }
        });

        // Butona tÄ±klayÄ±nca sayfanÄ±n en Ã¼stÃ¼ne git
        scrollBtn.addEventListener('click', () => {
            window.scrollTo({ top: 0, behavior: 'smooth' });
        });
    </script>


   
